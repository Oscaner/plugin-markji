{
  "name": "@maimemo-cli/plugin-markji",
  "version": "0.0.0",
  "description": "The markji plugin of maimemo-cli.",
  "author": "Oscaner Miao @Oscaner",
  "type": "module",
  "bin": {
    "markji": "./bin/run.js"
  },
  "homepage": "https://github.com/maimemo-cli/plugin-markji",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": "./dist/index.js",
  "repository": "maimemo-cli/plugin-markji",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "fs-extra": "^11.1.0",
    "got": "^12.5.3",
    "lodash": "^4.17.21",
    "user-agents": "^1.0.1225",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.12",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/got": "^9.6.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.7",
    "@types/user-agents": "^1.0.2",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "commands": "./dist/commands",
    "scope": "maimemo-cli",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "bin": "maimemo",
    "topics": {
      "markji": {
        "description": "Manage markji assets interactively"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/maimemo-cli/plugin-markji/issues",
  "keywords": [
    "oclif",
    "maimemo",
    "maimemo-cli",
    "markji",
    "plugin-markji"
  ],
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
